"[0-9]+": int
"[0-9]*\.[0-9]+|[0-9]+\.[0-9]": float
"([0-9]*\.[0-9]+|[0-9]+\.[0-9]*|[0-9])(e|E)[\+|\-]?[0-9]+": scientific
"for|while|end": loops
"\[|\]": matrix
"(figure|plot|plot3|xlabel|ylabel|zlabel|axis|legend|set|hold|pause)(?=\()": plot
"\.\.\.": dots
"format": format
"(?<=format ).*(?=\n)": formatThis
"close|clear|clc": clean
"(?<=(close|clear) ).*(?=\n)" cleanThis
"\=|\+|\-|\>|\<|\*|\/": operators
"true|false": bools
"\%.*(?=\n)": comment
"\'.*\'": string
